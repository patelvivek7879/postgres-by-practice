datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
  previewFeatures = ["fullTextSearch", "fullTextIndex"]
}

model users {
  user_id   String? @unique
  username  String?
  password String
  progress Int?
  email String @unique
  name String?
  dob DateTime?
  picture String?
  accesstoken String?
}

model feedbacks {
  f_id String? @unique
  from String
  name String?
  message String?
  createdAt BigInt @unique
}
