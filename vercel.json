{
  "version": 2,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "/pbyp"
    }
  ],
  "builds": [
    { "src": "server.ts", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/v1/(.*)", "dest": "/server.ts" },
    { "handle": "filesystem" }
  ]
}
