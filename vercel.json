{
  "version": 2,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/"
    },
    { 
      "source": "/api/v1/(.*)", "destination": "/api/v1/$1" 
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server.ts",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTION"]
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "/pbyp"
    }
  ]
}
