{
    "version": 2,
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/"
      }
    ],
    "redirects": [
      {
        "source": "/",
        "destination": "/pbyp"
      }
    ],
    "routes": [
      {
          "src": "/api/v1/(.*)",
          "dest": "./dist/server.js"
      }
  ]
  }
  