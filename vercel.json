{
  "version": 2,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "/pbyp"
    }
  ],
  "routes": [
    { "src": "/api/v1","methods": ["GET","POST","PATCH","PUT","OPTIONS"] , "dest": "/server.js" }
  ]
}
